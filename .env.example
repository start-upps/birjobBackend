# Database Configuration
DATABASE_URL=postgresql+asyncpg://your_db_user:your_db_password@your-database-host.aws.neon.tech/your_database_name?sslmode=require

# Gemini API Key
GEMINI_API_KEY=your_gemini_api_key_here

# Redis Configuration  
REDIS_URL=redis://your-redis-instance.upstash.io:6379
UPSTASH_REDIS_REST_URL="https://your-redis-instance.upstash.io"
UPSTASH_REDIS_REST_TOKEN="your_upstash_redis_token_here"

# Security
SECRET_KEY=your_secret_key_here
API_KEY=your-api-key-here

# CORS
ALLOWED_ORIGINS=["*"]

# Apple Push Notifications
APNS_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----
your_apns_private_key_content_here
-----END PRIVATE KEY-----"
APN_PATH=/etc/secrets/apn.p8
APNS_KEY_ID=your_apns_key_id
APNS_TEAM_ID=your_apns_team_id
APNS_BUNDLE_ID=com.yourcompany.yourapp
APNS_SANDBOX=true

# App Settings
LOG_LEVEL=INFO
MATCH_ENGINE_INTERVAL_MINUTES=5
MAX_NOTIFICATIONS_PER_HOUR=5
MAX_NOTIFICATIONS_PER_DAY=20