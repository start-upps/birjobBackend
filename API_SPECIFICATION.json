  "components": {
    "schemas": {
      "DeviceInfo": {
        "type": "object",
        "properties": {
          "osVersion": {
            "type": "string",
            "description": "iOS version"
          },
          "appVersion": {
            "type": "string",
            "description": "App version"
          },
          "deviceModel": {
            "type": "string",
            "description": "Device model identifier"
          },
          "timezone": {
            "type": "string",
            "description": "Device timezone"
          }
        },
        "required": ["osVersion", "appVersion", "deviceModel", "timezone"]
      },
      "Job": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          },
          "title": {
            "type": "string"
          },
          "company": {
            "type": "string"
          },
          "apply_link": {
            "type": "string",
            "format": "uri"
          },
          "source": {
            "type": "string"
          },
          "posted_at": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "JobMatch": {
        "type": "object",
        "properties": {
          "match_id": {
            "type": "string",
            "format": "uuid"
          },
          "job": {
            "$ref": "#/components/schemas/Job"
          },
          "matched_keywords": {
