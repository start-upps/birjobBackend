                        "subscription_id": "660e8400-e29b-41d4-a716-446655440001",
                        "keywords": ["iOS Developer", "Swift", "React Native"],
                        "sources": ["linkedin", "indeed"],
                        "location_filters": {
                          "cities": ["New York"],
                          "remote_only": false
                        },
                        "created_at": "2025-06-14T08:00:00Z",
